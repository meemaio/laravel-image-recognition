{
    "name": "meema/laravel-image-recognition",
    "description": "This package a Laravel wrapper around image recognition services.",
    "keywords": [
        "meema",
        "cion",
        "laravel",
        "aws",
        "rekognition",
        "google cloud",
        "vision",
        "laravel-image-recognition"
    ],
    "homepage": "https://github.com/meemaio/laravel-image-recognition",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Breuer",
            "email": "chris@cion.agency",
            "homepage": "https://cion.agency",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Meema\\LaravelImageRecognition\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meema\\LaravelImageRecognition\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meema\\LaravelImageRecognition\\ImageRecognitionServiceProvider"
            ],
            "aliases": {
                "Recognize": "Meema\\LaravelImageRecognition\\ImageRecognitionFacade"
            }
        }
    }
}
